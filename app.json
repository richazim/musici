{
  "expo": {
    "name": "Music-App",
    "slug": "musici",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconn.png",
    "userInterfaceStyle": "dark",
    "scheme": "Music-App",
    "splash": {
      "image": "./assets/splashhscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saibou.player",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splashhscreen.png",
        "backgroundColor": "#000000"
      },
      "package": "music.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsConfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4dc945a1-50e6-4037-a097-de66c50edcfc"
      }
    },
    "owner": "saibou"
  }
}
